#!/usr/bin/env bash

set -eux

# Copy common utils
mkdir -p ${BOSH_INSTALL_TARGET}/common
cp -a ${BOSH_COMPILE_TARGET}/common/* ${BOSH_INSTALL_TARGET}/common

# Extract nats_exporter package
mkdir -p ${BOSH_INSTALL_TARGET}/bin
tar xzvf ${BOSH_COMPILE_TARGET}/nats_exporter/prometheus-nats-exporter-v0.12.0-linux-x86_64.tar.gz -C ${BOSH_INSTALL_TARGET}/bin
